#include "main.h"
int main(void)
{
	char *command;
	char *command_path;
	char **tokens;
	
	while(1)
	{
		print_prompt();
		command = read_line();
		tokens = parse_line(command);
		command_path = find_executable(tokens);
		child_process(command_path, tokens);
	}
	free(command_path);
	free_tokens(tokens);
	free(command);
	return (0);
}
